{
  "short_name": "React App",
  "name": "Worklist extension",
  "description": "Override the new tab page with your own todolist",
  "version": "0.3",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "topSites",
    "notifications",
    "contextMenus"
  ]
}